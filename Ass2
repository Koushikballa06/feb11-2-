sno=int(input('enter sno'))
sname=input('enter sname')
s1=int(input('enter s1 marks'))
s2=int(input('enter s2 marks'))
s3=int(input('enter s3 marks'))
total = s1+s2+s3
avg = total/3
if avg>=91:
   print('o grade')
elif avg >=81:
   print('A grade')
elif avg>=71:
    print('B grade')
elif avg>=60:
    print('c grade')
elif avg>=50:
    print ('d grade')
elif avg>=40:
    print('pass')
else:
print('fail')
